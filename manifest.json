{
    "manifest_version": 3,
    "name": "To do List",
    "version": "1.0.0",
    "description": "Add stuff to it",
    "icons": {
      "128": "icon128.png",
      "48": "icon48.png",
      "16": "icon16.png"
    },
    "action": {
      "default_icon": "icon16.png",
      "default_popup": "index.html"
    },
    "permissions": ["scripting", "tabs", "activeTab"]
  }